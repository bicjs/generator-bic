{
  "name": "<%= name %>",
  "title": "<%= title %>",
  "description": "<%= description %>",
  "version": "0.0.0",
  "author": {
    "name": "<%= author %>",
    "email": "<%= email %>"
  },
  "bugs": "https://github.com/<%= username %>/<%= name %>/issues",
