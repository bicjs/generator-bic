{
  "name": "<%= name %>",
  "description": "<%= description %>",
  "version": "0.0.0",
  "author": {
    "name": "<%= author %>",
    "email": "<%= email %>"
  },
  "bugs": "https://github.com/<%= username %>/<%= name %>/issues",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "DOCS_DIR": "./docs"
  },
  "contributors": [
    {
      "name": "<%= author %>",
      "email": "<%= email %>"
    }
  ],
  "dependencies": {
    "@bicjs/bic": "^0.0.0-1",
    "@bicjs/bic-logger": "^0.0.0-1",
    "@bicjs/bic-template": "^0.0.0-1",
    "app-root-path": "^1.0.0",
    "basic-auth": "^1.0.3",
    "bootstrap-sass": "3.3.5",
    "bower": "^1.6.5",
    "compression": "^1.6.0",
    "csso": "^1.4.1",
    "errorhandler": "^1.4.2",
    "express": "^4.13.3",
    "express-cache-response-directive": "^0.2.0",
    "express-conditional-middleware": "^1.0.4",
    "faker": "^3.0.1",
    "fs-extra": "^0.26.2",
    "html-minifier": "^1.0.0",
    "img-placeholder-src": "^0.2.4",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "normalize.css": "^3.0.3",
    "npm-link-shared": "^0.2.0",
    "uglify-js": "^2.5.0"
  },
  "devDependencies": {
    "blue-tape": "^0.1.10",
    "bump-cli": "^1.1.3",
    "cz-conventional-changelog": "^1.1.4",
    "faucet": "0.0.1",
    "fixpack": "^2.2.0",
    "license-checker": "4.2.0",
    "license-generator": "0.0.13",
    "node-readme": "^0.1.9",
    "npm-check": "4.1.3",
    "npm-shrinkwrap": "200.4.0",
    "symlink": "2.1.0"
  },
  "engines": {
    "node": "5.1.x",
    "npm": "3.3.x"
  },
  "homepage": "https://github.com/<%= username %>/<%= name %>#readme",
  "keywords": [
    "bic",
    "demo",
    "framework",
    "website"
  ],
  "license": "<%= license %>",
  "main": "app",
  "repository": "github:<%= username %>/<%= name %>",
  "scripts": {
    "bower": "bower install",
    "buildpacks": "heroku buildpacks:set <%= buildpack %> --app=<%= name %> && heroku buildpacks:set <%= buildpack %> --app=<%= name %>-staging",
    "deps:check": "npm-check --update",
    "dev": "bic sequence dev",
    "heroku": "foreman start",
    "license": "npm run license:generator & npm run license:checker",
    "license:checker": "license-checker --customPath ./license-checker.json --out ./docs/licenses.md --markdown",
    "license:generator": "license-generator install <%= license %> -y <%= year %> -n <%= author %> -p <%= name %>",
    "link": "symlink ../ --execute",
    "logs": "heroku logs --app=<%= name %> --tail",
    "logs:staging": "heroku logs --app=<%= name %>-staging --tail",
    "postinstall": "npm run bower & npm run license && npm run readme",
    "postlink": "npm run postinstall",
    "prelink": "npm i symlink",
    "prestart": "bic sequence --release deploy",
    "readme": "node-readme",
    "shrinkwrap": "npm-shrinkwrap",
    "start": "node app --release",
    "test": "faucet",
    "vars:decrypt": "bic decrypt",
    "vars:encrypt": "bic encrypt"
  },
  "title": "<%= title %>"
}
