    {
      "name": "<%= author %>",
      "email": "<%= email %>"
    }
  ],
  "dependencies": {
    "@flickmy/bic": "file:../bic",
    "@flickmy/bic-logger": "file:../bic-logger",
    "@flickmy/bic-template": "file:../bic-template",
    "app-root-path": "^1.0.0",
    "basic-auth": "^1.0.3",
    "bower": "^1.6.5",
    "compression": "^1.5.2",
    "csso": "^1.3.11",
    "errorhandler": "^1.4.2",
    "express": "^4.13.3",
    "express-cache-response-directive": "^0.2.0",
    "express-conditional-middleware": "^1.0.3",
    "faker": "^3.0.1",
    "fs-extra": "^0.24.0",
    "html-minifier": "^0.7.2",
    "img-placeholder-src": "^0.2.4",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "normalize.css": "^3.0.3",
    "uglify-js": "^2.4.24"
  },
  "devDependencies": {
    "blue-tape": "^0.1.10",
    "bump-cli": "^1.1.3",
    "faucet": "0.0.1",
